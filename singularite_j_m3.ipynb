   "source": [
    "from itertools import product\n",
    "from singularite import group, isJStrict\n",
    "\n",
    "@interact\n",
    "def vis3d(n = (2, 100)):\n",
    "    pts = [p for p in product(group(n), repeat=3) if isJStrict(n, p)]\n",
    "    return sum ( point(p, size=100) for p in pts )"
   ]
  }
